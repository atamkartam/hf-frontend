{
    "builds": [
      {
        "src": "huggingface-backend/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "huggingface-frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "huggingface-backend/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "huggingface-frontend/dist/$1"
      }
    ]
  }